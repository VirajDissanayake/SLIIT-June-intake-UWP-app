        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:WPF="clr-namespace:DotNetBrowser.WPF;assembly=DotNetBrowser"
        Title="MainWindow" Height="472.343" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25*"/>
            <RowDefinition Height="90"/>
        </Grid.RowDefinitions>
        <WPF:WPFBrowserView Name="browserView" Grid.Row="0" URL="http://html5demos.com/dnd-upload"/>
        <TextBox Name="Output" HorizontalAlignment="Stretch" Grid.Row="1" VerticalAlignment="Stretch" IsReadOnly="True" AllowDrop="False" IsUndoEnabled="False" VerticalScrollBarVisibility="Auto"/>
